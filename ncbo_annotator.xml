<tool id="ncbo_annotator" name="Annotates a text with bio-ontology terms" version="1.0.1">
	<description>Annotates biological texts with the terms of the bio-ontologies from BioPortal</description>
	<command>java -jar ${__tool_data_path__}/shared/jars/ncbo_annotator_galaxy.jar $longestOnly $wholeWordOnly $filterNumber $stopWords $withDefaultStopWords $isTopWordsCaseSensitive $mintermSize $scored $withSynonyms $ontologiesToExpand $ontologiesToKeepInResult $semanticTypes $levelMax $mappingTypes $textToAnnotate $format $apikey > $output </command>
